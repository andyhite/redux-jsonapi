  "license": "MIT",
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setup.jest.js"
    ],
    "testPathDirs": [
      "./example",
      "./src"
    ],
    "testRegex": "\\.test\\.js$"
  }
